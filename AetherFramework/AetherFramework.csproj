<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>A simple and lightweight modding framework.</Description>
    <PackageProjectUrl>https://sanicbtw.github.io/aether-framework</PackageProjectUrl>
    <RepositoryUrl>https://github.com/SanicBTW/Aether-Framework</RepositoryUrl>
    <Copyright>xhyabunny © 2024</Copyright>
    <PackageTags>Modding, Framework</PackageTags>
    <PackageReleaseNotes>Overhauled the Class Registry system alongside some small improvements.</PackageReleaseNotes>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <SignAssembly>False</SignAssembly>
    <Authors>sanco</Authors>
    <Company>xhyabunny</Company>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<VersionPrefix>1.1.0</VersionPrefix>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.1.0.0</FileVersion>
  </PropertyGroup>

</Project>
